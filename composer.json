  ],
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "job-runner/job-runner": "^0.2",
    "symfony/console": "^6.3.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^12.0",
    "phpunit/phpunit": "^10.2.1",
    "psalm/plugin-phpunit": "^0.18.4",
    "vimeo/psalm": "^5.12"
  },
  "autoload": {
    "psr-4": {
      "JobRunner\\JobRunner\\SymfonyConsole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JobRunner\\JobRunner\\SymfonyConsole\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "infection/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
